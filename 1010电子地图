//floyd复杂的太高，不合适，用dij
#include<bits/stdc++.h>
using namespace std;
int n,m,s,t,a;
int mp[1005][1005];
void floyd(){
	for(int k=1;k<=n;k++)
	for(int i=1;i<=n;i++)
	for(int j=1;j<=n;j++){
		if(mp[i][k]+mp[k][j]<mp[i][j])mp[i][j]=mp[i][k]+mp[k][j];
	}
}
int main(){
	while(scanf("%d %d %d %d %d",&n,&m,&s,&t,&a)!=EOF){
		int f,g,h;
		for(int i=1;i<=n;i++)
		for(int j=1;j<=n;j++){
			if(i==j)mp[i][j]=0;
			else mp[i][j]=1e9;
		}
		for(int i=1;i<=m;i++){
			scanf("%d %d %d",&f,&g,&h);
			if(f%2==0){
				if(g%2==0){
					mp[f][g]=mp[g][f]=h+2;
				}
				else{
					mp[f][g]=h+2;
					mp[g][f]=h+1;
				}
			}
			else {
				if(g%2==0) {
					mp[f][g]=h+1;
					mp[g][f]=h+2;
				}
				else{
					mp[f][g]=mp[g][f]=h+1;
				}
			}
		}
		floyd();
		if(mp[s][t]<=a){
			printf("YES %d\n",mp[s][t]);
		}
		else printf("KENG\n");
	}
}
